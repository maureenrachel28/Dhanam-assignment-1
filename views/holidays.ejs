<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <table>

        <thead>
        <tr>
            <th>Holidays</th>
            <th>Dates</th>
        </tr>
        </thead>
    
        <tbody>
            <%filteredEvents.forEach(function(item){%>
                <tr>
                    <td><%=item.title%></td>
                    <td><%=item.date%></td>
                </tr>
            <%})%>
        </tbody> 
        </table>
       
</body>
</html>